@{

    if (User.Identity.IsAuthenticated)
    {
        if (User.IsInRole("0"))
        {
            Layout = "~/Views/Shared/_LayoutUser.cshtml";
        }
        else
        {
            Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
        }

    }
    Layout = "~/Views/Shared/_Layout.cshtml";
}
